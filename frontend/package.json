{
  "name": "project-management-blog-frontend",
  "version": "1.0.0",
  "description": "Vue.js frontend for Project Management Blog",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "docker:build": "docker build -t pm-blog-frontend .",
    "docker:run": "docker run -p 3000:3000 pm-blog-frontend",
    "analyze": "vite-bundle-analyzer dist/stats.html",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "pinia": "^2.3.0",
    "axios": "^1.7.9",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "chart.js": "^4.4.7",
    "vue-chartjs": "^5.3.1",
    "@vueuse/core": "^13.6.0",
    "dayjs": "^1.11.13",
    "vue-toastification": "^2.0.0-rc.5",
    "prismjs": "^1.29.0",
    "markdown-it": "^14.1.0",
    "dompurify": "^3.2.2",
    "vue-meta": "^3.0.0-alpha.2",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.3",
    "@vue/test-utils": "^2.4.3",
    "vitest": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^25.0.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.31.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.2",
    "vue-tsc": "^2.1.10",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash-es": "^4.17.12",
    "@types/dompurify": "^3.0.5",
    "@types/markdown-it": "^14.1.2",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-vue-components": "^0.27.4",
    "vite-bundle-analyzer": "^0.8.0",
    "@storybook/vue3": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/testing-library": "^0.2.2",
    "storybook": "^8.4.7",
    "c8": "^10.1.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "vue",
    "vite",
    "tailwindcss",
    "typescript",
    "frontend",
    "blog",
    "project-management"
  ],
  "author": "Rey Nieves <reyisnieves@example.com>",
  "license": "MIT"
}
